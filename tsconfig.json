{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "allowJs": false,
    "esModuleInterop": true,
    "jsx": "react-native",
    "lib": ["DOM", "ESNext"],
    "moduleResolution": "node",
    "noEmit": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "target": "ESNext",
    "strict": true,
    "typeRoots": ["node_modules/@types", "./src/types"]

  }
}
